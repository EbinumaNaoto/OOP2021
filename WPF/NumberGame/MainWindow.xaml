<Window x:Class="NumberGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NumberGame"
        x:Name="MainForm"
        mc:Ignorable="d"
        Title="数字当てゲーム" Width="800" Height="800" Loaded="Window_Loaded">
    <StackPanel>
        <Grid x:Name="gridPanel">
            <TextBlock x:Name="answerText" Height="30" FontSize="15"
                       TextWrapping="Wrap" Text="TextBlock" Margin="0,738,0,-738"/>
        </Grid>
    </StackPanel>
</Window>
